{
    "menu": {
        "class": "Menu\\Services\\Menu",
        "arguments": [
            "@router",
            "@config",
            "@query"
        ]
    },
    "menu.hook.app": {
        "class": "Menu\\Services\\HookApp",
        "arguments": [
            "@core",
            "@query"
        ],
        "hooks": {
            "app.response.after": "hookResponseAfter",
            "menu.show.response.after": "hookMenuShowResponseAfter"
        }
    }
}