{
    "user.login": {
        "methode": "GET",
        "path": "user/login:url",
        "uses": "Login@formLogin",
        "with": {
            ":url": "(/\\w+)?"
        }
    },
    "user.login.check": {
        "methode": "POST",
        "path": "user/login:url",
        "uses": "Login@loginCheck",
        "with": {
            ":url": "(/\\w+)?"
        }
    },
    "user.relogin": {
        "methode": "GET",
        "path": "user/relogin:url",
        "uses": "Login@relogin",
        "with": {
            ":url": "(/\\w+)?"
        }
    },
    "user.relogin.check": {
        "methode": "POST",
        "path": "user/relogin:url",
        "uses": "Login@reloginCheck",
        "with": {
            ":url": "(/\\w+)?"
        }
    },
    "user.reset": {
        "methode": "GET",
        "path": "user/:id/reset/:token",
        "uses": "Login@resetUser",
        "with": {
            ":id": "\\d+",
            ":token": "[0-9a-zA-Z]+"
        }
    },
    "user.logout": {
        "methode": "GET",
        "path": "user/logout",
        "uses": "Login@logout"
    }
}