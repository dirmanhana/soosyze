{
    "user.role.admin": {
        "methode": "GET",
        "path": "admin/user/role",
        "uses": "Role@admin"
    },
    "user.role.create": {
        "methode": "GET",
        "path": "user/role",
        "uses": "Role@create"
    },
    "user.role.store": {
        "methode": "POST",
        "path": "user/role",
        "uses": "Role@store"
    },
    "user.role.edit": {
        "methode": "GET",
        "path": "user/role/:id/edit",
        "uses": "Role@edit",
        "with": {
            ":id": "\\d+"
        }
    },
    "user.role.update": {
        "methode": "POST",
        "path": "user/role/:id/edit",
        "uses": "Role@udpate",
        "with": {
            ":id": "\\d+"
        }
    },
    "user.role.remove": {
        "methode": "GET",
        "path": "user/role/:id/delete",
        "uses": "Role@remove",
        "with": {
            ":id": "\\d+"
        }
    },
    "user.role.delete": {
        "methode": "POST",
        "path": "user/role/:id/delete",
        "uses": "Role@delete",
        "with": {
            ":id": "\\d+"
        }
    }
}