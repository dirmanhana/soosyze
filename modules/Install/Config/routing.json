{
    "install.index": {
        "methode": "GET",
        "path": "/",
        "uses": "Install@index"
    },
    "install.step": {
        "methode": "GET",
        "path": "install/step/:id",
        "uses": "Install@step",
        "with": {
            ":id": "\\d+"
        }
    },
    "install.step.check": {
        "methode": "POST",
        "path": "install/step/:id",
        "uses": "Install@stepCheck",
        "with": {
            ":id": "\\d+"
        }
    }
}