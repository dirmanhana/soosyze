{
    "module": {
        "class": "System\\Services\\Modules",
        "arguments": [
            "@query"
        ]
    },
    "system.hook": {
        "class": "System\\Services\\System",
        "arguments": [
            "@router",
            "@config",
            "@template",
            "@user"
        ],
        "hooks": {
            "app.404": "hooks404",
            "app.403": "hooks403",
            "app.reponse.before": "hookSys",
            "app.reponse.after": "hookMeta"
        }
    },
    "system.install": {
        "class": "System\\Install",
        "hooks": {
            "install.user": "hookInstallUser",
            "install.menu": "hookInstallMenu"
        }
    }
}