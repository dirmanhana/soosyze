{
    "user.login": {
        "methode": "GET",
        "path": "user/login",
        "uses": "User@login"
    },
    "user.login.check": {
        "methode": "POST",
        "path": "user/login",
        "uses": "User@loginCheck"
    },
    "user.relogin": {
        "methode": "GET",
        "path": "user/relogin",
        "uses": "User@relogin"
    },
    "user.relogin.check": {
        "methode": "POST",
        "path": "user/relogin",
        "uses": "User@reloginCheck"
    },
    "user.reset": {
        "methode": "POST",
        "path": "user/:id/reset/:token",
        "uses": "User@resetUser",
        "with": {
            ":id": "[0-9]+",
            ":token": "[0-9a-zA-Z]+"
        }
    },
    "user.logout": {
        "methode": "GET",
        "path": "user/logout",
        "uses": "User@logout"
    },
    "user.views": {
        "methode": "GET",
        "path": "user/:id",
        "uses": "User@views",
        "with": {
            ":id": "[0-9]+"
        }
    },
    "user.edit": {
        "methode": "GET",
        "path": "user/:id/edit",
        "uses": "User@edit",
        "with": {
            ":id": "[0-9]+"
        }
    },
    "user.edit.check": {
        "methode": "POST",
        "path": "user/:id/edit",
        "uses": "User@editCheck",
        "with": {
            ":id": "[0-9]+"
        }
    }
}